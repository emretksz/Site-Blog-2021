            <div class="row portfolio-container aos-init aos-animate" data-aos="fade-up" data-aos-easing="ease-in-out" data-aos-duration="500" style="position: relative; height: 877.5px;">

                @foreach (var portfolio in Model.Portfolios)
                {
                    <div class="col-lg-4 col-md-6 filter-@portfolio.PortfolioCategories.Name" style="position: absolute; left: 0px; top: 0px;">
                        <div class="portfolio-item">
                            <img src="@portfolio.ImageURL" class="img-fluid" alt="">
                            <div class="portfolio-info">
                                <h3><a href="@portfolio.ImageURL" data-gall="portfolioGallery" class="venobox vbox-item" title="@portfolio.Name">@portfolio.Name</a></h3>
                                <div>
                                    <a href="@portfolio.ImageURL" data-gall="portfolioGallery" class="venobox vbox-item" title="@portfolio.Name"><i class="bx bx-plus"></i></a>
                                    <a href="@Url.Action("PortfolioDetails","Home",new {id=portfolio.PortfolioId })" title="Portfolio Details"><i class="bx bx-link"></i></a>
                                </div>
                            </div>
                        </div>
                    </div>
                }

